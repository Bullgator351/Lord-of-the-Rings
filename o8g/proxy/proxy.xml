  <template src="proxy/hero.jpg" default="True">
    <matches>
      <match name="Type" value="Hero" />
    </matches>
    <overlayblocks>
		<conditional>
		  <switch property="Sphere">
		    <case value="Fellowship">
				<link block="fellowship_hero" />
			</case>
			<case value="Baggins">
				<link block="baggins_hero" />
			</case>
			<case value="Tactics">
				<link block="tactics_hero" />
			</case>
			<case value="Leadership">
				<link block="leadership_hero" />
			</case>
			<case value="Lore">
				<link block="lore_hero" />
			</case>
			<case value="Spirit">
				<link block="spirit_hero" />
			</case>
		  </switch>
		</conditional>
		<link block="dummy" />
    </overlayblocks>
    <textblocks>
	  <link block="hero_cost">
	   <property name="Cost" />
	  </link>
	  <link block="character_name" separator="">
	   <property name="Unique" />
	   <property name="Name" />
	  </link>
	  <link block="character_willpower">
	   <property name="Willpower" />
	  </link>
	  <link block="character_attack">
	   <property name="Attack" />
	  </link>
	  <link block="character_defense">
	   <property name="Defense" />
	  </link>
	  <link block="character_hitpoints">
	   <property name="Health" />
	  </link>
	  <link block="traits">
	   <property name="Traits" />
	  </link>
	  <link block="description" separator="&#10;">
	   <property name="Keywords" />
	   <property name="Text" />
	  </link>
	  <link block="victory">
	   <property name="Victory Points" />
